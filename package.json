{
    "name": "veenun",
    "version": "1.0.0",
    "description": "Cross Browser VersionOne Extension",
    "main": "",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cmunky/veenun.git"
    },
    "author": "cmunky",
    "license": "GPL-2.0",
    "bugs": {
        "url": "https://github.com/cmunky/veenun/issues"
    },
    "homepage": "https://github.com/cmunky/veenun#readme",
    "devDependencies": {
        "grunt": "^0.4.5",
        "grunt-jsbeautifier": "^0.2.10"
    },
    "buildConfig": {
        "mozilla": {
            "pkg": ["title", "name", "version", "description", "license", "author"],
            "main": "background-mozilla.js",
            "engines": {
                "firefox": ">=38.0a1",
                "fennec": ">=38.0a1"
            },
            "script": ["events.js", "page-firefox.js"]
        },
        "webkit": {
            "pkg": ["name", "version", "description"],
            "manifest_version": 2,
            "permissions": ["tabs", "alarms", "storage",
                "http://localhost/", "http://wintermute/"
            ],
            "background": {
                "scripts": [
                    "./js/jquery.min.js",
                    "background-webkit.js"
                ]
            },
            "web_accessible_resources": [
                "data/template.html",
                "data/config-webkit.json",
                "img/*.png",
                "css/*.css"
            ],
            "script": ["page-chrome.js"]
        },
        "shared": {
            "target": ["https://www13.v1host.com/GettyImagesEnterprise/*"],
            "style": [
                "bootstrap-scoped.min.css",
                "spectrum.css",
                "style.css"
            ],
            "script": [
                "jquery.min.js",
                "bootstrap.min.js",
                "plates.js",
                "spectrum.js",
                "common.js"
            ],
            "options": {
                "remote": "http://wintermute:23232",
                "template": "template.html",
                "branchUrl": "branch-32.png",
                "gearUrl": "gear-32.png",
                "plusUrl": "plus-24.png",
                "colors": [
                    "#C1BEE6", "#DDDF60", "#60E2BD", "#F0A487", "#BBDD99", "#59E0EB", "#EBB85B",
                    "#82E695", "#EAAECE", "#B0E175", "#9ACEE2", "#9CD7C0", "#D0C679"
                ]
            }
        }
    }
}
