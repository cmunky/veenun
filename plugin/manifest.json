{
	"name": "veenun",
	"version": "1.0.0",
	"manifest_version": 2,
  "permissions": ["tabs", "alarms", "storage",
    "http://localhost/", "http://wintermute/"  ],
  "background": { "scripts": ["./js/jquery.min.js", "background-webkit.js"] },
  "content_scripts": [
    {
      "matches": ["https://www13.v1host.com/GettyImagesEnterprise/*"],
      "css": ["./css/bootstrap-scoped.min.css", "./css/spectrum.css", "./css/style.css"],
      "js": [ "./js/jquery.min.js", "./js/bootstrap.min.js", "js/plates.js", "js/spectrum.js", "./js/common.js", "./js/page-chrome.js" ]
    }
  ],
  "web_accessible_resources": [
    "data/template.html",
    "data/config-webkit.json",
    "img/*.png",
    "css/*.css"
  ],
  "description": "Cross Platform VersionOne Browser Extension"
}
